#include <iostream>
#include <string>
using namespace std;
int main()
{
    string text = "#include <iostream> \n"
                   " \n"
                   "int main() { \n"
                   "    std::cout << \"Hello, World!\\n\" << std::endl; \n"
                   "    return 0; \n"
                   "}";
    cout<<text;
}
