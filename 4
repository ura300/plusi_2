#include <iostream>
#include <string>
using namespace std;
int main()
{
    system("chcp 65001");
    int d, c;
    cout<<"Введите десятичное число: "<<endl;
    cin>>d;
    string bin;
    while (d>0)
    {
        c = d%2;
        bin = to_string(c) + bin;
        d=d/2;
    }
    cout<<bin;
    return 0;
}
