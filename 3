#include <iostream>
using namespace std;
int main() {
    int a;
    cout<< "Enter binary number:\n";
    cin >> a;
    int b, c, m;
    b = 0;
    c = 0;
    m = 1;
    while(a>0){
        b = a % 10;
        c = c + b * m;
        m = m * 2;
        a = a / 10;
    }
   cout << c << endl;
    return 0;
}
