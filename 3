#include <iostream>
#include <cmath>
using namespace std;
int main() {
    system("chcp 65001");
    int bin;
    cout<< "Введите двоичное число:\n";
    cin >> bin;
    int b, c, d, st;
    b = 0;
    c = 0;
    d = 0;
    while(bin > 0){
        b = bin % 10;
        if(b == 1){
        st = pow(2,d);
        c += st;
        d += 1;
        bin = bin / 10;
        }

        if(b == 0){
        d += 1;
        bin = bin / 10;
        }
    }
    cout << c << endl;
    return 0;
}
