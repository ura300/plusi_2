#include <iostream>
#include <string>
using namespace std;
int main()
{
    system("chcp 65001");
    string word = "темных";
    //cout<< "Введите слово: " << endl;
    //cin>>word;
    string line = "возьми этих темных душ, да бойся древней крови";
    //cout<< "Введите строку: " << endl;
    //cin>>line;
    size_t n  = line.find(word);
    if (line.find(word)!=string::npos)
    {
        cout << word << endl;
        cout << n << endl;
    }
    return 0;

}
